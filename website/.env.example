# Development/staging API endpoint
# Used when building the website for dev/staging Docker deployment
# Browser will connect to API at this URL
VITE_DECENT_CLOUD_API_URL=http://localhost:59011

# Stripe API publishable key (for frontend)
# Get your test keys from https://dashboard.stripe.com/test/apikeys
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Chatwoot widget configuration (optional)
# Get from Chatwoot inbox settings after deployment
VITE_CHATWOOT_WEBSITE_TOKEN=
VITE_CHATWOOT_BASE_URL=https://support.decent-cloud.org
