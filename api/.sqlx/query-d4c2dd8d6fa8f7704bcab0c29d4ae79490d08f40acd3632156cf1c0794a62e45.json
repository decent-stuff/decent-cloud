  "query": "SELECT\n                ep.pubkey,\n                ep.name,\n                ep.domain,\n                ep.website_url,\n                ep.logo_url,\n                ep.data_source,\n                ep.created_at_ns,\n                COUNT(po.id) as offerings_count\n            FROM external_providers ep\n            LEFT JOIN provider_offerings po ON ep.pubkey = po.pubkey AND po.offering_source = 'seeded'\n            GROUP BY ep.pubkey\n            ORDER BY ep.name",
