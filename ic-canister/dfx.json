{
  "canisters": {
    "decent_cloud": {
      "candid": "decent_cloud.did",
      "package": "decent_cloud_canister",
      "type": "rust"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "mainnet-01": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    },
    "mainnet-eu": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8002",
      "type": "ephemeral",
      "replica": {
        "log_level": "info"
      }
    }
  },
  "output_env_file": ".env",
  "version": 1
}
